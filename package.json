{
	"name": "rhelp",
	"displayName": "rHelp",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:rhelp.showHelp",
		"*"
	],
	"repository": {
		"type": "github",
		"url": "https://github.com/ManuelHentschel/vscode-R-help"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rhelp.showHelp",
				"title": "R Help: Show help"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/express": "^4.17.8",
		"@types/glob": "^7.1.3",
		"@types/jsdom": "^16.2.5",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"@types/highlight.js": "^9.12.4",
		"@types/showdown": "^1.9.3",
		"express": "^4.17.1",
		"highlight.js": "^10.3.1",
		"jsdom": "^16.4.0",
		"showdown": "^1.9.1"
	}
}
